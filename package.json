{
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p watch:* serve",
    "serve": "live-server public --host=localhost --port=3000 --no-browser",
    "compile:babel": "babel src -d public/js --source-maps",
    "watch:babel": "npm run compile:babel -- --watch",
    "build:babel": "npm run compile:babel -- --minified"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-react": "7.18.6",
    "live-server": "1.2.2",
    "npm-run-all": "4.1.5"
  },
  "babel": {
    "comments": false,
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  }
}