{
  "private": true,
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  },
  "scripts": {
    "dev": "npm run serve",
    "serve": "webpack serve --node-env=development -c webpack/dev.js",
    "bundle": "webpack --node-env=development -c webpack/dev.js",
    "build": "webpack build --node-env=production -c webpack/build.js"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-react": "7.18.6",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.7.1",
    "style-loader": "3.3.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "browserslist": [
